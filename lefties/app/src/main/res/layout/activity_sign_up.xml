<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/signUpConstraintView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EF6E6C"
    android:contentDescription="@string/signUpScroll"
    tools:context=".SignUpActivity">

    <ImageView
        android:id="@+id/signUpAssetOne"
        android:layout_width="411dp"
        android:layout_height="344dp"
        android:layout_marginBottom="130dp"
        android:contentDescription="@string/signUpAssetOne"
        app:layout_constraintBottom_toTopOf="@+id/signUpAssetTwo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sign_up_sset_one"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/signUpAssetTwo"
        android:layout_width="256dp"
        android:layout_height="279dp"
        android:layout_marginStart="186dp"
        android:layout_marginBottom="-50dp"
        android:contentDescription="@string/signUpAssetTwo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/sign_up_asset_two"
        tools:ignore="ImageContrastCheck" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/logo_brandmark_maroon"
            tools:layout_editor_absoluteX="99dp"
            tools:layout_editor_absoluteY="16dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Lefties"
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="39dp" />

        <TextView
            android:id="@+id/accountName"
            android:layout_width="350dp"
            android:layout_height="45dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="67dp"
            android:layout_marginEnd="31dp"
            android:layout_marginBottom="69dp"
            android:background="@color/white"
            android:hint="Name"
            app:layout_constraintBottom_toTopOf="@+id/accountEmail"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView4" />

        <EditText
            android:id="@+id/accountPassword"
            android:layout_width="350dp"
            android:layout_height="45dp"
            android:background="@color/white"
            android:ems="10"
            android:hint="Password"
            android:inputType="textPassword"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="277dp" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="58dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="56dp"
            android:minHeight="45dp"
            android:spinnerMode="dialog"
            app:layout_constraintBottom_toBottomOf="@+id/accountEmail"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/accountName" />

        <EditText
            android:id="@+id/editTextTextMultiLine"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="124dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="124dp"
            android:background="@color/white"
            android:ems="10"
            android:gravity="start|top"
            android:hint="Address"
            android:inputType="textMultiLine"
            android:minHeight="55dp"
            app:layout_constraintBottom_toBottomOf="@+id/button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/accountEmail" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="68dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="66dp"
            android:background="@color/white"
            android:text="Plain text 3"
            app:layout_constraintBottom_toBottomOf="@+id/editTextTextMultiLine"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/accountEmail" />

        <EditText
            android:id="@+id/accountEmail"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="220dp"
            android:layout_marginEnd="30dp"
            android:background="@color/white"
            android:ems="10"
            android:hint="Email"
            android:inputType="textEmailAddress"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/accountCity"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="64dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="70dp"
            android:background="@color/white"
            android:hint="City"
            app:layout_constraintBottom_toBottomOf="@+id/button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/editTextTextMultiLine" />

        <Button
            android:id="@+id/button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="91dp"
            android:scrollbars="vertical"
            android:text="Scrollable Button"
            android:textColor="#B71C1C"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>